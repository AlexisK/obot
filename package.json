    "inert": "^4.0.1",
    "jira-client": "^3.0.2",
    "lodash": "^4.14.0",
    "orm": "^3.1.0",
    "pg": "^6.0.3",
    "rand-token": "^0.2.1",
    "sequelize": "^3.23.6",
    "simple-ssh": "^0.9.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
