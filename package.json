{
  "name": "slackbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:server:w\" \"npm run tsc:client:w\" \"npm run nodemon:w\" \"npm run gulp:client:w\"",
    "tsc:w": "tsc --watch",
    "nodemon:w":"node_modules/.bin/nodemon --watch build/app.js --delay 0.1 build/app.js",
    "gulp:client:w":"gulp build-cli",
    "tsc:server:w": "tsc --p tsconfig.server.json --watch",
    "tsc:client:w": "tsc --p tsconfig.client.json --watch",
    "postinstall": "typings install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
